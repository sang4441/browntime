<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.browntime"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
         >
         <activity android:name=".CollectionDemoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BrownPagerActivity"
            android:label="@string/app_name"
            android:parentActivityName=".CollectionDemoActivity"  >
        </activity>
        <activity
            android:name=".BrownMenuActivity"
            android:label="@string/app_name" 
            >
        </activity>
        <activity
            android:name=".BrownCartListActivity"
            android:label="@string/app_name"
            android:parentActivityName=".CollectionDemoActivity" > 
        </activity>
       <activity
            android:name=".BrownOrderActivity"
            android:label="@string/app_name"> 
        </activity>
    </application>
</manifest>
